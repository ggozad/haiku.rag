# haiku.rag configuration for ag-ui-research example
# Copy to haiku.rag.yaml and customize

research:
  provider: ollama
  model: gpt-oss:latest
  max_iterations: 3
  confidence_threshold: 0.8
  max_concurrency: 1

providers:
  ollama:
    base_url: http://host.docker.internal:11434

